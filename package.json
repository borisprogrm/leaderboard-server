    "@nestjs/config": "^3.2.1",
    "@nestjs/core": "^10.3.7",
    "@nestjs/platform-express": "^10.3.7",
    "@nestjs/swagger": "^7.3.1",
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.10",
    "@types/pg": "^8.11.0",
    "express-openapi-validator": "^5.1.6",
    "http-terminator": "^3.2.0",
    "ioredis": "^5.3.2",
