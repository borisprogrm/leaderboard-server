  "version": "0.9.0",
  "description": "HTTP server providing leaderboard RESTful API",
  "main": "dist/app.js",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint --cache . --ext .ts",
    "lint-fix": "eslint --cache . --ext .ts --fix",
    "test": "jest --runInBand",
    "test:quick": "jest --runTestsByPath tests/app.test.ts --coverage=false --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/borisprogrm/leaderboard-server.git"
  },
  "keywords": [
    "leaderboard",
    "http",
    "nestjs",
    "openapi",
    "typescript",
    "jest"
  ],
  "author": {
    "name": "Boris Panov"
  },
  "license": "ISC",
  "homepage": "https://github.com/borisprogrm/leaderboard-server#readme",
  "engines": {
    "node": ">=20.11.1 <21"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.3.7",
    "@types/jest": "^29.5.7",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "ioredis-mock": "^8.9.0",
    "jest": "^29.7.0",
    "jest-dynalite": "^3.6.1",
    "supertest": "^6.3.3",
